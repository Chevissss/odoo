<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Secuencia para Canchas -->
        <record id="seq_cancha" model="ir.sequence">
            <field name="name">Secuencia Canchas</field>
            <field name="code">reserva.cancha</field>
            <field name="prefix">CANCHA-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <!-- Secuencia para Reservas -->
        <record id="seq_reserva" model="ir.sequence">
            <field name="name">Secuencia Reservas</field>
            <field name="code">reserva.reserva</field>
            <field name="prefix">RES-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>
